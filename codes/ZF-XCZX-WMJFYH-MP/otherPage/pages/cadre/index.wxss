/* otherPage/pages/cadre/index.wxss */
.page {
    min-height: 100vh;
    box-sizing: border-box;
    position: relative;
    background: #f6f6f6;
    padding-bottom: 1rpx;
}

.bg {
    position: absolute;
    top: -150rpx;
    left: 0;
    right: 0;
    width: 100%;
}

.topBox {
    color: #ffffff;
    margin-bottom: 20rpx;
    position: relative;
}

.topBox .tit {
    font-size: 36rpx;
    font-weight: 500;
    margin-bottom: 10rpx;
}

.topBox .dec {
    font-size: 32rpx;
    font-weight: 500;
}

.topBox .btn {
    width: 120rpx;
    min-width: 120rpx;
    height: 120rpx;
    background: rgba(255, 255, 255, 0.20);
    border-radius: 20rpx;
    text-align: center;
    font-size: 24rpx;
    margin-left: 20rpx;
    border: 1rpx solid rgba(255, 255, 255, .5);
}

.topBox .btn image {
    width: 60rpx;
    height: 60rpx;
}

.page-content {
    position: relative;
    padding: 25rpx 30rpx 0rpx;
    box-sizing: border-box;
}

.page-top {
    margin-bottom: 30rpx;
    margin-left: -15rpx;
    margin-right: -15rpx;
}

.page-top .item {
    /* width: 210rpx; */
    flex: 1;
    margin: 0 15rpx;
    /* height: 320rpx; */
    background: linear-gradient(180deg, #ffffff, rgba(255, 255, 255, 0.85) 100%);
    border-radius: 20rpx;
    text-align: center;
    padding: 40rpx 10rpx 10rpx;
    box-sizing: border-box;
    font-size: 28rpx;
}

.page-top .item .icon {
    width: 96rpx;
    margin-bottom: 20rpx;
}

.page-top .item .tit {
    margin-bottom: 5rpx;
}

.page-top .item .numBox {
    font-size: 24rpx;
    margin-bottom: 15rpx;
}

.page-top .item .numBox .num {
    font-weight: 700;
    font-size: 40rpx;
    color: #000000;
}

.page-top .item .goDetail {
    height: 48rpx;
    background: #eaf9f4;
    border-radius: 10rpx;
    font-size: 28rpx;
    color: #0ab481;
}

.title {
    padding-left: 30rpx;
    position: relative;
    font-size: 32rpx;
    font-weight: 600;
}

.title::before {
    content: '';
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
    width: 8rpx;
    height: 32rpx;
    background: #0ab481;
    border-radius: 8rpx;
}

.data-analysis .tabBox {
    width: 360rpx;
    height: 56rpx;
    background: #ffffff;
    border-radius: 30rpx;
}

.data-analysis .tabBox .tabTit {
    width: 50%;
    border: 2rpx solid #cccccc;
    font-size: 28rpx;
    color: #000;
}

.data-analysis .tabBox .tabTit.on {
    border-color: #0ab481;
    color: #0ab481;
}

.data-analysis .tabBox .tabTit1 {
    border-radius: 28rpx 0rpx 0rpx 28rpx;
    border-right: none;
}

.data-analysis .tabBox .tabTit2 {
    border-radius: 0rpx 28rpx 28rpx 0rpx;
    border-left: none;
}

.data-analysis .tabBox .xian {
    height: 100%;
    width: 2rpx;
    background: #0AB481;
}

.echarts {
    background: #fff;
    border-radius: 20rpx;
}

.data-analysis {
    margin-bottom: 30rpx;
}

.tabListBox .tabListTitBox {
    margin-bottom: 20rpx;
    /* height: 80rpx; */
    /* background: #ffffff; */
    /* border-radius: 40rpx; */
}

.tabListBox .tabListTitBox .item {
    width: 330rpx;
    padding: 10rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    color: #333333;
    border-radius: 30rpx;
}

.tabListBox .tabListTitBox .item.on {
    color: #0AB481;
    /* font-size: 32rpx; */
    font-weight: 500;
    position: relative;
}

.tabListBox .tabListTitBox .item.on::before {
    content: '';
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    bottom: 0;
    width: 100rpx;
    height: 6rpx;
    background: #0ab481;
    border-radius: 4rpx;
}

.tabListBox .dataNum {
    font-size: 32rpx;
    font-weight: 500;
    color: #000000;
    text-align: center;
    margin-bottom: 30rpx;
}

.tabListBox .dataNum text {
    color: #F82619;
}

.tabListCont .item {
    background: #ffffff;
    border-radius: 20rpx;
    padding: 30rpx 30rpx 0;
    box-sizing: border-box;
    margin-bottom: 20rpx;
}

.tabListCont .item .userInfo {
    padding-bottom: 20rpx;
    border-bottom: 2rpx solid #f6f6f6;
    margin-bottom: 20rpx;
}

.tabListCont .item .head {
    min-width: 80rpx;
    width: 80rpx;
    margin-right: 20rpx;
}

.tabListCont .item .name {
    font-size: 32rpx;
    color: #000000;
    font-weight: 500;
    margin-bottom: 10rpx;
}

.tabListCont .item .time {
    font-size: 24rpx;
    color: #000000;
}

.tabListCont .item .taskTit {
    background: rgba(10, 180, 129, 0.06);
    border-radius: 10rpx;
    padding: 16rpx;
    box-sizing: border-box;
    color: #0ab481;
}

.tabListCont .item .points {
    font-size: 36rpx;
    font-weight: 500;
    color: #ff3939;
}

.tabListCont .item .taskTit .tit {
    font-weight: 500;
    min-width: 90rpx;
}

.tabListCont .item .taskTit .text {
    color: #666666;
}

.tabListCont .item .taskTit .text text {
    color: #0AB481;
}

.taskClass {
    color: #666666;
    margin-bottom: 18rpx;
}

.taskClass .icon {
    width: 24rpx;
    margin-right: 10rpx;
}

.taskClass .tag {
    padding: 0 10rpx;
    height: 40rpx;
    background: linear-gradient(105deg, #18be8d, #3ad190 100%);
    border-radius: 10rpx;
    color: #fff;
    font-size: 24rpx;
}

.mar-b-30 {
    margin-bottom: 30rpx;
}

.mar-b-20 {
    margin-bottom: 20rpx;
}

.tabListCont .item .icon {
    min-width: 28rpx;
    width: 28rpx;
    margin-right: 20rpx;
}

.tabListCont .item .btns {
    border-top: 2rpx solid #f6f6f6;
    padding: 10rpx 0;
    box-sizing: border-box;
    font-size: 28rpx;
    font-weight: 500;
}

.tabListCont .item .btns .btn {
    width: 50%;
}

.tabListCont .item .btns .btn2 {
    color: #0ab481;
}

.tabListCont .item .btns .xian {
    width: 2rpx;
    height: 52rpx;
    background: #f6f6f6;
}

.tabListCont .item .picBox {
    margin-right: -20rpx;
}

.tabListCont .item .picBox image {
    width: 120rpx;
    height: 120rpx;
    border-radius: 10rpx;
    margin: 0 20rpx 20rpx 0;
}

.fbTit {
    font-weight: 500;
    color: #333;
    margin-bottom: 10rpx;
}

.fbTitInfo {
    color: #666;
    margin-bottom: 10rpx;
}

.fbTitInfo2 {
    color: #666;
    /* font-size: 24rpx; */
    margin-bottom: 30rpx;
}

.fbLabel {
    margin-bottom: 10rpx;
}

.icon2 {
    width: 28rpx;
    margin-right: 10rpx;
}

.fixedBox {
    width: 140rpx;
    /* height: 420rpx; */
    background: #0ab481;
    border-radius: 20rpx;
    box-shadow: 0rpx 4rpx 16rpx 0rpx rgba(0, 0, 0, 0.25);
    /* position: fixed;
    right: 20rpx;
    top: 40vh; */
    color: #fff;
    text-align: center;
    font-size: 36rpx;
    font-weight: 500;
    text-shadow: 4rpx 8rpx 8rpx 0rpx #b9e5d4;
    z-index: 2;
    text-align: center;
    font-size: 24rpx;

}

.fixedBox .item {
    height: 140rpx;
}

.fixedBox image {
    width: 64rpx;
    margin-bottom: 10rpx;
}

.fixedBox .xian {
    width: 100rpx;
    height: 0rpx;
    border-top: 2rpx solid rgba(255, 255, 255, 0.20);
    box-shadow: 0rpx 4rpx 16rpx 0rpx rgba(0, 0, 0, 0.25);
    margin: 0 auto;
}

