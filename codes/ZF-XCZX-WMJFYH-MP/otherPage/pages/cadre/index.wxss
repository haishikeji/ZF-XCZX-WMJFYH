.classChooseBtn {
    height: 80rpx;
    background: #ffffff;
    border-radius: 94rpx;
    margin-bottom: 28rpx;
    position: relative;
}

.arrow {
    color: #cccccc;
    font-size: 28rpx;
    position: absolute;
    right: 30rpx;
    top: 50%;
    transform: translateY(-50%) rotate(90deg);
}

movable-area {
    pointer-events: none;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
}

movable-view {
    pointer-events: all;
}

.tabListCont .item .userInfo2 {
    margin-bottom: 20rpx;
}

.tabListCont .item .userInfo2 .head {
    min-width: 180rpx;
    width: 180rpx;
    height: 240rpx;
    border-radius: 20rpx;
}

.infoItem {
    margin-bottom: 5rpx;
}